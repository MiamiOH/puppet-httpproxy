{
  "name": "MiamiOH-httpproxy",
  "version": "2.0.0",
  "author": "Michael Callahan, Chris Edester",
  "summary": "Provides a simple way to manage various proxies",
  "license": "GPL-3.0+",
  "source": "https://github.com/MiamiOH/puppet-httpproxy.git",
  "project_page": "https://github.com/MiamiOH/puppet-httpproxy",
  "issues_url": "https://github.com/MiamiOH/puppet-httpproxy/issues",
  "tags": ["proxy", "linux"],
  "dependencies": [
    {"name": "puppetlabs/stdlib", "version_requirement": ">= 4.25.0 < 6.0.0"},
    {"name": "puppetlabs/inifile", "version_requirement": ">= 1.3.0 < 4.0.0"},
    {"name": "unibet/profiled", "version_requirement": ">= 0.1.4 < 1.0.0"},
    {"name": "puppetlabs/apt", "version_requirement": ">= 2.2.0 < 8.0.0"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["5", "6", "7"]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": ["5", "6", "7"]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": ["5", "6", "7"]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": ["7", "8"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["12.04", "14.04"]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.0 < 6.0.0"
    }
  ]
}
